{
  "name": "@ohno/core",
  "author": {
    "name": "haozhe",
    "email": "sailist@outlook.com"
  },
  "version": "0.0.4",
  "main": "./dist/ohno.umd.js",
  "module": "./dist/ohno.esm.js",
  "unpkg": "./dist/ohno.umd.min.js",
  "types": "./dist/ohno.d.ts",
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "preview": "vite preview",
    "test": "vitest run",
    "debug": "vitest  --inspect-brk --no-threads",
    "coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "#lint": "biome lint .",
    "#clean": "rimraf dist out-tsc",
    "#build:api": "build-api --tsc tsconfig.lib.json",
    "#publint": "publint",
    "#prepack": "compat-exports",
    "#typecheck": "tsc -b"
  },
  "devDependencies": {
    "@edge-runtime/vm": "^2.1.2",
    "@types/katex": "^0.16.0",
    "@types/sanitize-html": "^2.9.0",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vitest/coverage-c8": "^0.29.8",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "happy-dom": "^9.1.0",
    "jsdom": "^21.1.1",
    "typescript": "^4.9.3",
    "vite-plugin-eslint": "^1.8.1",
    "config": "workspace:*"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.2.6",
    "highlight.js": "^11.7.0",
    "htm": "^3.1.1",
    "katex": "^0.16.4",
    "sanitize-html": "^2.10.0"
  },
  "files": [
    "./dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/ohno.d.mts",
        "default": "./dist/ohno.mjs"
      },
      "types": "./dist/ohno.d.ts",
      "module": "./dist/ohno.esm.js",
      "default": "./dist/ohno.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oh-note/ohno",
    "directory": "packages/core"
  }
}